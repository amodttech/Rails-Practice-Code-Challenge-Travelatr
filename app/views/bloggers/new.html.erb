
<ul>
    <% @blogger.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
</ul>
</div>


<%= form_for @blogger do |f| %>
    <%= f.label :name %><br>
    <%= f.text_field :name %><br>

    <%= f.label :bio %><br>
    <%= f.text_area :bio %><br>

    <%= f.label :age %><br>
    <%= f.text_field :age %><br>

    <%= f.submit %>
<% end %>